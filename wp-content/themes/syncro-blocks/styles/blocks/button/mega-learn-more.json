{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Mega Learn More",
	"slug": "mega-learn-more",
	"blockTypes": [ "core/button" ],
	"styles": {
		"spacing": {
			"padding": {
				"bottom": "0.5rem",
				"left": "0.75rem",
				"right": "0.75rem",
				"top": "0.5rem"
			}
		},
        "color": {
            "text": "var(--wp--preset--color--base)",
            "background": "var(--wp--custom--color--syncro-black-50)"
        },
        "css": "&:not(.has-background):hover{ background-color: var(--wp--custom--color--cream-4-50) !important; color: var(--wp--preset--color--base) !important; text-decoration: none !important;}"
	}
}