{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ghub/content-toggle",
  "version": "1.0.0",
  "title": "Content Toggle",
  "category": "ghub-products",
  "keywords": [
    "toggle",
    "content toggle"
  ],
  "description": "Use the Content Toggle block in Gutenberg to switch between two sets of content, such as primary/secondary or annual/monthly.",
  "attributes": {
    "inactiveClass": {
      "type": "string"
    },
    "activeClass": {
      "type": "string"
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "leftText": {
      "type": "string",
      "default": ""
    },
    "rightText": {
      "type": "string",
      "default": ""
    },
    "toggleBaseColor": {
      "type": "string",
      "default": "#cccccc"
    },
    "toggleActiveColor": {
      "type": "string",
      "default": "#F2ECFF"
    },
    "toggleTextColor": {
      "type": "string",
      "default": ""
    },
    "toggleActiveTextColor": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalTextDecoration": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "textdomain": "ghub-content-toggle",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "ghub-toggle-frontend"
}